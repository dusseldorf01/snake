{
  "name": "snake",
  "version": "1.0.0",
  "description": "old school snake",
  "main": "src/index.tsx",
  "scripts": {
    "start": "cross-env TS_NODE_PROJECT=\"tsconfig.webpack.json\" webpack serve --config webpack.client.config.ts --progress --hot",
    "start:ssr": "npm-run-all -s server:develop server:start",
    "build": "npm-run-all -s build:server build:client",
    "build:server": "npm run webpack:run -- --config webpack.server.config.ts --mode production",
    "build:client": "npm run webpack:run -- --config webpack.client.config.ts --mode production",
    "lint:ts": "eslint src --ext .ts --ext .tsx --fix",
    "lint:css": "stylelint src/**/*.css --fix",
    "docker:build": "docker build . -t cr.yandex/crp96j9lfkrv2cslsrdh/snake",
    "docker:push": "docker push cr.yandex/crp96j9lfkrv2cslsrdh/snake:latest",
    "docker:pull": "docker pull cr.yandex/crp96j9lfkrv2cslsrdh/snake:latest",
    "docker": "docker run -p 49150:8080 -d cr.yandex/crp96j9lfkrv2cslsrdh/snake",
    "server:develop": "npm run webpack:run -- --config webpack.server.config.ts --mode development",
    "server:start": "nodemon dist/server.js",
    "webpack:run": "cross-env TS_NODE_PROJECT=\"tsconfig.webpack.json\" webpack --progress",
    "server": "node server.js",
    "test:unit": "jest",
    "test:e2e": "start-server-and-test start http://localhost:8080 cypress",
    "cypress": "cypress run --browser chrome",
    "cypress:open": "cypress open"
  },
  "nodemonConfig": {
    "watch": [
      "dist/server.js",
      "webpack.client.config.ts",
      "webpack.server.config.ts"
    ],
    "delay": 1000
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.(ts|tsx)": "npm run lint:ts",
    "*.css": "npm run lint:css"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dusseldorf01/snake.git"
  },
