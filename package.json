{
  "name": "snake",
  "version": "1.0.0",
  "description": "old school snake",
  "main": "src/index.tsx",
  "scripts": {
    "start": "webpack serve --mode=development",
    "build": "webpack --mode=production",
    "lint:ts": "eslint src --ext .ts --ext .tsx --fix",
    "lint:css": "stylelint src/**/*.css --fix",
    "server": "node server.js",
    "docker:build": "docker build -t dusseldorf01/snake .",
    "docker": "docker run -p 49160:3000 -d dusseldorf01/snake"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.(ts|tsx)": "npm run lint:ts",
    "*.css": "npm run lint:css"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dusseldorf01/snake.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/dusseldorf01/snake/issues"
  },
  "homepage": "https://github.com/dusseldorf01/snake#readme",
  "devDependencies": {
    "@types/react": "17.0.0",
    "@types/react-dom": "17.0.0",
    "@types/react-redux": "7.1.12",
    "@types/react-router": "5.1.8",
    "@types/react-router-dom": "5.1.6",
    "@typescript-eslint/eslint-plugin": "4.9.1",
    "@typescript-eslint/parser": "4.8.2",
    "classnames": "2.2.6",
    "clean-webpack-plugin": "3.0.0",
    "copy-webpack-plugin": "7.0.0",
    "css-loader": "5.0.1",
    "css-minimizer-webpack-plugin": "1.1.5",
    "eslint": "7.14.0",
    "eslint-config-airbnb-typescript": "12.0.0",
    "eslint-plugin-import": "2.22.1",
    "eslint-plugin-jsx-a11y": "6.4.1",
    "eslint-plugin-react": "7.21.5",
    "eslint-plugin-react-hooks": "4.2.0",
    "html-loader": "1.3.2",
    "html-webpack-plugin": "5.0.0-alpha.15",
    "husky": "4.3.5",
    "lint-staged": "10.5.3",
    "mini-css-extract-plugin": "1.3.3",
    "postcss": "8.2.1",
    "postcss-for": "2.1.1",
    "postcss-import": "13.0.0",
    "postcss-loader": "4.1.0",
    "postcss-nested": "5.0.3",
    "postcss-simple-vars": "6.0.2",
